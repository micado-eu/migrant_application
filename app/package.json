{
  "name": "micado",
  "version": "0.0.1",
  "description": "Migrant application",
  "productName": "Micado app",
  "cordovaId": "org.cordova.quasar.app",
  "capacitorId": "",
  "author": "Luca Gioppo",
  "private": true,
  "scripts": {
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:debug": "jest --debug",
    "test:unit": "jest --updateSnapshot",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "test:unit:ui": "majestic",
    "test:e2e": "cypress open",
    "test:e2e:CI": "cypress run"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^4.0.0",
    "@quasar/extras": "^1.0.0",
    "@uppy/core": "^1.6.0",
    "@uppy/dashboard": "^1.5.0",
    "@uppy/tus": "^1.5.0",
    "annyang": "^2.6.1",
    "axios": "^0.18.0",
    "axios-jsonp": "^1.0.4",
    "jsonapi-vuex": "^3.1.1",
    "leaflet": "^1.6.0",
    "quasar": "^1.0.0",
    "uppy": "^1.6.0",
    "validator": "^11.1.0",
    "vue-cytoscape": "^1.0.7",
    "vue-i18n": "^7.3.3",
    "vue-mermaid": "0.0.10",
    "vue2-leaflet": "^2.2.1",
    "vuelidate": "^0.7.4"
  },
  "devDependencies": {
    "@quasar/app": "^1.0.0",
    "@quasar/quasar-app-extension-testing": "^1.0.0",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^1.0.0-beta.10",
    "@quasar/quasar-app-extension-testing-unit-jest": "^1.0.0",
    "exports-loader": "^0.7.0",
    "majestic": "^1.2.24",
    "quasar-app-extension-auth-token-based": "^0.3.1",
    "wallaby-vue-compiler": "^1.0.3"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ]
}
